  },
  "devDependencies": {
    "@types/webpack-merge": "^4.1.5",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arzyu/airpack"
  },
